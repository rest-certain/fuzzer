{
    "name": "rest-certain/fuzzer",
    "description": "A testing utility for generating valid and invalid API request data.",
    "license": "LGPL-3.0-or-later",
    "type": "library",
    "keywords": [
        "api",
        "schema",
        "json",
        "fuzzer",
        "test",
        "rest"
    ],
    "authors": [
        {
            "name": "Ben Ramsey",
            "email": "ben@ramsey.dev",
            "homepage": "https://ben.ramsey.dev"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "ramsey/devtools": "^2.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "RestCertain\\Fuzzer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RestCertain\\Test\\Fuzzer\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "captainhook/plugin-composer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "ramsey/composer-repl": true,
            "ramsey/devtools": true
        },
        "sort-packages": true
    },
    "extra": {
        "captainhook": {
            "force-install": true
        },
        "ramsey/conventional-commits": {
            "configFile": "conventional-commits.json"
        },
        "ramsey/devtools": {
            "command-prefix": "dev",
            "memory-limit": "-1"
        }
    }
}
